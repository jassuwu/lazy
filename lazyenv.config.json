{
  "src": {
    "path": "~/repos/p2pdotme/contracts-v3/contract-addresses.json",
    "cmd": "bunx hardhat run scripts/deployContracts.ts --network baseSepolia"
  },
  "dest": {
    "paths": [
      "~/repos/personal/broker-ui/.env",
      "~/repos/personal/user-app/.env",
      "~/repos/personal/merchant-app/.env"
    ],
    "envMapping": {
      "merchantRegistryAddress": "VITE_MERCHANT_REGISTRY",
      "orderProcessorAddress": "VITE_ORDER_PROCESSOR",
      "p2pxConfigAddress": "VITE_CONFIG_CONTRACT",
      "reputationManagerAddress": "VITE_REPUTATION_MANAGER",
      "usdtAddress": "VITE_USDT_CONTRACT_ADDRESS"
    }
  }
}
